{
  "name": "heroku-logger",
  "version": "0.0.2",
  "description": "A dead simple logger, designed to be perfect for Heroku apps.",
  "repository": "git://github.com/contentshq/heroku-logger.git",
  "main": "./lib/index.js",
  "dependencies": {
    "chalk": "^1.1.3",
    "flatten-obj": "^3.1.0",
    "is": "^3.2.1",
    "logfmt": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "mocha": "^3.2.0",
    "np": "^2.13.1"
  },
  "scripts": {
    "clean": "rm -rf ./lib ./node_modules",
    "build": "babel --out-dir ./lib ./src",
    "prepublish": "npm run build",
    "release": "np",
    "test": "npm run build && npm run tests",
    "tests": "mocha --compilers js:babel-core/register"
  },
  "keywords": [
    "bunyan",
    "heroku",
    "log",
    "logfmt",
    "logger",
    "logging",
    "winston"
  ]
}
